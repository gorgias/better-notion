{
  "name": "Notion",
  "version": "0.0.1",
  "description": "The Notion Extension helps.",
  "manifest_version": 3,
  "icons": {
    "16": "icons/notion_16x16.png",
    "32": "icons/notion_32x32.png",
    "48": "icons/notion_48x48.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/notion_16x16.png",
      "32": "icons/notion_32x32.png",
      "48": "icons/notion_48x48.png"
    }
  },
  "host_permissions": [
    "https://www.notion.so/*"
  ],  
  "permissions": [
    "storage",
    "identity",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.notion.so/*"
      ],
      "run_at": "document_end",
      "all_frames": true,
      "css": [
        "css/custom.css"
      ],
      "js": [
        "constantMessage.js",
        "lib/js/jquery-3.5.1.min.js",
        "contentScript/contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [],
  "content_security_policy": {}
}
